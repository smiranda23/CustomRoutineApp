CREATE TABLE TrainingEntity (
        id INTEGER PRIMARY KEY AUTOINCREMENT,
        name TEXT NOT NULL,
        duration INTEGER NOT NULL,
        date TEXT NOT NULL,
        setsFinished INTEGER NOT NULL
);

insert:
INSERT INTO TrainingEntity (name,duration, date, setsFinished)
VALUES
(?,?,?,?);

delete:
DELETE FROM TrainingEntity WHERE id = ?;

selectAll:
SELECT * FROM TrainingEntity;